{
  "name": "tobacco-map",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "comma-separated-values": "^3.6.4",
    "core-js": "^2.5.7",
    "d3-array": "^1.2.4",
    "d3-axis": "^1.0.12",
    "d3-collection": "^1.0.7",
    "d3-force": "^1.1.2",
    "d3-format": "^1.3.2",
    "d3-geo": "^1.11.1",
    "d3-geo-projection": "^2.4.1",
    "d3-hierarchy": "^1.1.9",
    "d3-request": "^1.0.6",
    "d3-scale": "^1.0.7",
    "d3-scale-chromatic": "^1.3.3",
    "d3-selection": "^1.3.2",
    "d3-shape": "^1.2.2",
    "d3-time-format": "^2.1.3",
    "d3-transition": "^1.1.3",
    "d3-voronoi": "^1.1.4",
    "i18next": "^10.6.0",
    "lodash.isdate": "^4.0.1",
    "lodash.isnumber": "^3.0.3",
    "lodash.isstring": "^4.0.1",
    "nouislider": "^11.1.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-scripts": "^1.1.5",
    "tinycolor2": "^1.4.1",
    "topojson-client": "^3.0.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./node_modules ./src/styles/index.scss -o ./public",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules ./src/styles/index.scss -o ./public --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "make-svgs": "babel-node scripts/tosvg.js",
    "gather-data": "babel-node scripts/gatherdata.js",
    "build-css-standalone": "node-sass-chokidar --include-path ./node_modules ./src/styles/index.standalone.scss ./public/ta-visualizations.css",
    "build-standalone": "npm run build-css-standalone && webpack",
    "build-js-standalone-watch": "webpack --config=webpack.dev.config.js --watch --progress",
    "build-css-standalone-watch": "node-sass-chokidar --include-path ./node_modules ./src/styles/index.standalone.scss ./public/ta-visualizations.css --watch --recursive",
    "build-standalone-watch": "npm-run-all -p build-css-standalone-watch build-js-standalone-watch"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "copy-webpack-plugin": "^4.6.0",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.3",
    "puppeteer": "^0.12.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^3.12.0"
  }
}
